    "orbit = xo.orbits.KeplerianOrbit(\n",
    "    period=input_p,\n",
    "    t0=input_ttrans,\n",
    "    b=input_b,\n",
    "    ecc=input_ecc,\n",
    "    rstar=input_Rs,\n",
    "    mstar=input_Ms,\n",
    "    m_planet=input_mp,\n",
    "    m_planet_units=u.Mjup,\n",
    "    omega=input_omega,\n",
    "    Omega=input_Omega,\n",
    ")\n",
    "\n",
    "t0_ra=orbit.get_star_position(Time(2015.5,format='jyear').jd,parallax=input_plx)[0].eval()\n",
    "t0_dec=orbit.get_star_position(Time(2015.5,format='jyear').jd,parallax=input_plx)[1].eval()\n",
    "\n",
    "true_ra=orbit.get_star_position(gaia_t.jd,parallax=input_plx)[0].eval()\n",
