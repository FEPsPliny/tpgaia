   "outputs": [],
   "source": [
    "input_p  = 542.0\n",
    "input_mp = 5\n",
    "input_ecc = 0.1\n",
    "input_omega = 0.82*np.pi\n",
    "input_Omega = 1.25127*np.pi\n",
    "input_ttrans = 2460000.136\n",
    "input_b = 0.4\n",
    "input_Rs = 1.0\n",
    "input_Ms = 1.0\n",
    "input_Gmag = 9.0\n",
    "input_plx = 1000/30\n",
    "RA_deg = 37.1234\n",
    "DEC_deg = -39.183"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "id": "98e5f07e-f086-45c7-a5af-a5325df6bcf7",
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "\n",
    "_degtorad = np.pi/180\n",
    "_radtomas = (180 * 3600 * 1000) / np.pi\n",
    "_mastorad = np.pi / (180 * 3600 * 1000)\n",
    "_kmps_to_aupyr = (u.year.to(u.s) * u.km.to(u.m)) / constants.au\n",
    "\n",
    "\n",
    "url = f\"https://gaia.esac.esa.int/gost/GostServlet?ra=\"+str(RA_deg)+\"&dec=\"+str(DEC_deg)+\"&service=1\"\n",
    "with requests.Session() as s:\n",
    "    s.get(url)\n",
    "    headers = {\"Cookie\": f\"JSESSIONID={s.cookies.get_dict()['JSESSIONID']}\"}\n",
    "    response = s.get(url, headers=headers, timeout=180)\n",
    "    dat = response.text\n",
    "\n",
    "root = ET.fromstring(dat)\n",
    "columns = [\"Target\", \"ra[rad]\", \"dec[rad]\", \"ra[h:m:s]\", \"dec[d:m:s]\", \"ObservationTimeAtGaia[UTC]\",\n",
    "            \"CcdRow[1-7]\", \"zetaFieldAngle[rad]\", \"scanAngle[rad]\", \"Fov[FovP=preceding/FovF=following]\",\n",
    "            \"parallaxFactorAlongScan\", \"parallaxFactorAcrossScan\", \"ObservationTimeAtBarycentre[BarycentricJulianDateInTCB]\"]\n",
    "rows = []\n",
    "name = root.find('./targets/target/name').text\n",
    "raR = root.find('./targets/target/coords/ra').text\n",
    "decR = root.find('./targets/target/coords/dec').text\n",
    "raH = root.find('./targets/target/coords/raHms').text\n",
    "decH = root.find('./targets/target/coords/decDms').text\n",
    "for event in root.findall('./targets/target/events/event'):\n",
    "    details = event.find('details')\n",
    "    observationTimeAtGaia = event.find('eventUtcDate').text\n",
    "    ccdRow = details.find('ccdRow').text\n",
    "    zetaFieldAngle = details.find('zetaFieldAngle').text\n",
    "    scanAngle = details.find('scanAngle').text\n",
    "    fov = details.find('fov').text\n",
    "    parallaxFactorAl = details.find('parallaxFactorAl').text\n",
    "    parallaxFactorAc = details.find('parallaxFactorAc').text\n",
    "    observationTimeAtBarycentre = event.find('eventTcbBarycentricJulianDateAtBarycentre').text\n",
    "    rows.append([name, raR, decR, raH, decH, observationTimeAtGaia, ccdRow,\n",
    "                  zetaFieldAngle, scanAngle, fov, parallaxFactorAl, parallaxFactorAc, observationTimeAtBarycentre])\n",
    "gaia_obs_data = pd.DataFrame(rows, columns=columns)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "d24d0eef-9d8b-4fba-be2d-ec2775d3792e",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_Gaia_error(GAIAmag):\n",
    "    \"\"\"\n",
    "    Using the interpolation function to estimate along scan RMS error.\n",
    "    \"\"\"\n",
    "    \n",
    "    ast_std=pd.read_csv(\"~/python/tpgaia/Gaia_AlongScan_Uncertainties.csv\",header=None)\n",
    "    interp=interpolate.interp1d(ast_std[0].values,ast_std[1].values)\n",
    "    return interp(GAIAmag)\n",
    "    \n",
    "# The Gaia data is now loaded in as \"gaia_obs_data\" - accessing most important columns:\n",
    "gaia_t=Time(gaia_obs_data[\"ObservationTimeAtBarycentre[BarycentricJulianDateInTCB]\"].values.astype(float),format='jd')\n",
    "gaia_scanAng_rad=gaia_obs_data[\"scanAngle[rad]\"].values\n",
    "\n",
    "# Normal triad, defined at the reference epoch.\n",
    "p = np.array([-np.sin(RA_deg*_degtorad), np.cos(RA_deg*_degtorad), 0.0])\n",
    "q = np.array([-np.sin(DEC_deg*_degtorad) * np.cos(RA_deg*_degtorad), -np.sin(DEC_deg*_degtorad) * np.sin(RA_deg*_degtorad), np.cos(DEC_deg*_degtorad)])\n",
    "r = np.array([np.cos(DEC_deg*_degtorad) * np.cos(RA_deg*_degtorad), np.cos(DEC_deg*_degtorad) * np.sin(RA_deg*_degtorad), np.sin(DEC_deg*_degtorad)])\n",
    "\n",
    "bO_bcrs = earth_sun_l2_ephemeris(gaia_t.jyear)\n",
    "\n",
    "# Calculate the Roemer delay, take units into account.\n",
    "tB = gaia_t + np.dot(r, bO_bcrs) * constants.au / constants.c / u.year.to(u.s)\n",
    "uO_init = np.repeat(r, gaia_t.size).reshape((r.size, gaia_t.size))\n",
    "\n",
    "gaia_tfine = Time(np.arange(np.min(gaia_t).jd,np.max(gaia_t).jd,1),format='jd')\n",
    "bO_bcrs_fine=earth_sun_l2_ephemeris(gaia_tfine.jyear)\n",
    "tB_fine = gaia_tfine + np.dot(r, bO_bcrs_fine) * constants.au / constants.c / u.year.to(u.s)\n",
    "uO_init_fine = np.repeat(r, gaia_tfine.size).reshape((r.size, gaia_tfine.size))\n",
    "\n",
    "#Also initialising the expected Gaia error:\n",
    "gaia_error = get_Gaia_error(input_Gmag)\n"
   ]
  },
