   "source": [
    "\n",
    "random = np.random.default_rng(5678)\n",
    "t_plot = np.linspace(0, 22 * 365.25, 500)\n",
    "t = np.sort(random.uniform(0.0, 5 * 365.25, 45))\n",
    "rho_err = random.uniform(0.05, 0.1, len(t))\n",
    "theta_err = random.uniform(0.05, 0.1, len(t))\n",
    "\n",
    "import pymc as pm\n",
    "import pymc_ext as pmx\n",
    "with pm.Model():\n",
    "\n",
    "    # Period, semi-major axis, eccentricity, and t0\n",
    "    period = pm.Normal(\"period\", mu=np.random.normal(input_p,0.01), sigma=0.02)\n",
    "    ecc = xo.distributions.eccentricity.kipping13(\"ecc\",fixed=True)\n",
    "    t0 = pm.Normal(\"tperi\", mu=np.random.normal(input_ttrans,0.01), sigma=0.01)\n",
    "\n",
    "    # For astrometric orbits, a good choice of parameterization can be\n",
    "    # (Omega  omega) / 2\n",
    "    plus = pmx.angle(\"plus\")\n",
    "    minus = pmx.angle(\"minus\")\n",
    "    Omega = pm.Deterministic(\"Omega\", plus + minus)\n",
    "    omega = pm.Deterministic(\"omega\", plus - minus)\n",
    "\n",
    "    # We'll use a uniform prior on cos(incl)\n",
    "    b = pm.Uniform(\"cos_incl\", lower=0, upper=1.0)\n",
    "\n",
    "    mstar=pm.Normal(\"Mstar\",mu=1.0,sigma=0.1)\n",
    "    rstar=pm.Normal(\"Rstar\",mu=1.0,sigma=0.01)\n",
    "\n",
    "    logm_planet = pm.Uniform(\"logmpl\", lower=np.log(0.25), upper=np.log(150))\n",
    "    m_planet = pm.Deterministic(\"mpl\", pm.math.exp(logm_planet))\n",
    "    \n",
    "    ra  = pm.Normal(\"ra\",mu=RA_deg,sigma=0.0001)\n",
    "    dec = pm.Normal(\"dec\",mu=DEC_deg,sigma=0.0001)\n",
    "    # Then we define the orbit\n",
    "    orbit = xo.orbits.KeplerianOrbit(\n",
    "        t0=t0,\n",
    "        period=period,\n",
    "        b=b,\n",
    "        mstar=mstar,\n",
    "        rstar=rstar,\n",
    "        ecc=ecc,\n",
    "        omega=omega,\n",
    "        Omega=Omega,\n",
    "    )\n",
    "\n",
    "    ra_at_t0=pm.Deterministic(\"ra_at_t0\", orbit.get_star_position(Time(2015.5,format='jyear').jd, parallax=input_plx)[0])\n",
    "    dec_at_t0=pm.Deterministic(\"dec_at_t0\", orbit.get_star_position(Time(2015.5,format='jyear').jd, parallax=input_plx)[1])\n",
    "    \n",
    "    ra_offset_from_bary = pm.Deterministic(\"ra_offset_from_bary\", orbit.get_star_position(gaia_t.jd,parallax=input_plx)[0])\n",
    "    dec_offset_from_bary = pm.Deterministic(\"dec_offset_from_bary\", orbit.get_star_position(gaia_t.jd,parallax=input_plx)[1])\n",
    "\n",
    "    ra_offset_from_t0 = pm.Deterministic(\"ra_offset_from_t0\", ra_offset_from_bary-ra_at_t0)\n",
    "    dec_offset_from_t0 = pm.Deterministic(\"dec_offset_from_t0\", dec_offset_from_bary-ra_at_t0)\n",
    "\n",
    "    AL_offset = pm.Deterministic(\"AL_offset\", (ra_offset_from_t0)*pm.math.sin(gaia_scanAng_rad.astype(float))+(dec_offset_from_t0)*pm.math.cos(gaia_scanAng_rad.astype(float)))\n",
    "    \n",
    "    pm.Normal(\n",
    "        \"theta_obs\",\n",
    "        mu=AL_offset,\n",
    "        sigma=gaia_error,\n",
    "        observed=alongscan_y,\n",
    "    )\n",
    "\n",
    "    init_soln = pm.find_MAP()\n",
    "#     trace = pm.sample(\n",
    "#         tune=1000,\n",
    "#         draws=1000,\n",
    "#         cores=2,\n",
    "#         chains=2,\n",
    "#         target_accept=0.95,\n",
    "#         init=\"adapt_full\",\n",
    "#     )"
   ]
